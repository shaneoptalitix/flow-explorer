{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "coreclr",
      "request": "launch",
      "name": "Launch AzureDevOpsBuildsApi",
      "program": "${workspaceFolder}/AzureDevOpsBuildsApi/bin/Debug/net8.0/${input:executableName}",
      "args": [],
      "cwd": "${workspaceFolder}/AzureDevOpsBuildsApi",
      "stopAtEntry": false,
      "launchBrowser": {
        "enabled": true,
        "args": "swagger",
        "windows": {
          "command": "cmd.exe",
          "args": "/C start ${auto-detect-url}"
        },
        "osx": {
          "command": "open"
        },
        "linux": {
          "command": "xdg-open"
        }
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "preLaunchTask": "build"
    }
  ],
  "inputs": [
    {
      "id": "executableName",
      "type": "pickString",
      "description": "Select the .NET executable to debug",
      "options": ["AzureDevOpsBuildsApi.dll"]
    }
  ]
}